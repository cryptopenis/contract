    paletteId;

    svg = abi.encodePacked(svg,
      bytes('<g id="ball">'),
      balls[ballId],
      bytes('</g><g id="head">'),
      heads[headId],
      bytes('</g><g id="wing">'),
      wings[wingId]);

    svg = abi.encodePacked(svg,
      bytes('</g><g id="eye">'),
      eyes[eyeId],
      bytes('</g><g id="hat">'),
      hats[hatId],
      bytes('</g>'),
      SVG_FOOTER);
    return svg;
  }
