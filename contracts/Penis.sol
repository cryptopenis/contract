contract Penis is ERC721Royalty, PullPayment, Ownable {

  PenisCollection private penisCollection = new PenisCollection();
  uint8[6][] private tokens;
  uint256 private nonOwnerTokensMinted;
  AggregatorV3Interface internal priceFeed;

  uint16 lotSize = 3000;
  uint256 price = 10000000000; // in dollars, with 10 decimals
  uint256 priceGrowthRateNumerator = 161803398;
  uint256 priceGrowthRateDenominator = 100000000;

  constructor() ERC721("CryptoPenis", "CPNS") {
    // 690 / 1000 = 6.9%
    _setDefaultRoyalty(owner(), 690);
    // 0xAB594600376Ec9fD91F8e885dADF0CE036862dE0
    priceFeed = AggregatorV3Interface(0xAB594600376Ec9fD91F8e885dADF0CE036862dE0);
  }

  function _mintPrice() internal returns (uint256) {
    if (nonOwnerTokensMinted >= lotSize) {
      lotSize = lotSize + (lotSize * 9 / 10);
      price = price * priceGrowthRateNumerator / priceGrowthRateDenominator;
    }
    (,int dollarRate,,,) = priceFeed.latestRoundData(); // 8 decimals
    // 1 eth = 1 000 000 000 000 000 000 wei
    return price * uint(dollarRate);
  }
  
  function mint(uint8 ballId, uint8 headId, uint8 wingId, uint8 eyeId, uint8 hatId, uint8 paletteId
  ) public payable returns (uint256) {
      require(msg.value >= _mintPrice(), "Pay at least 0.05 ETH to mint");
      require(_possibleChoice(ballId, headId, wingId, eyeId, hatId, paletteId), "Asset does not exist");
      require(!_minted(ballId, headId, wingId, eyeId, hatId, paletteId), "This token was already minted");
      uint tokenId = tokens.length;
      _safeMint(msg.sender, tokenId);
      tokens.push([ballId, headId, wingId, eyeId, hatId, paletteId]);
      nonOwnerTokensMinted += 1;
      return uint256(tokenId);
  }

  function mintOwnerToken(uint8 ballId, uint8 headId, uint8 wingId, uint8 eyeId, uint8 hatId, uint8 paletteId
  ) public payable onlyOwner returns(uint256) {
      require(_canOwnerMint(), "Unable to mint to contract owner. More non-owner penises must be minted first");
      require(_possibleChoice(ballId, headId, wingId, eyeId, hatId, paletteId), "Asset does not exist");
      require(!_minted(ballId, headId, wingId, eyeId, hatId, paletteId), "This token was already minted");
      uint tokenId = tokens.length;
      tokens.push([ballId, headId, wingId, eyeId, hatId, paletteId]);
      _safeMint(msg.sender, tokenId);
      return uint256(tokenId);
  }

  function mintBatch(uint8 ballId, uint8 headId, uint8 wingId, uint8 eyeId, uint8 hatId, uint8 paletteId
  ) public payable returns(uint256) {
      require(msg.value >= 50000000000000000, "Pay at least 0.05 ETH to mint");
      require(_possibleChoice(ballId, headId, wingId, eyeId, hatId, paletteId), "Asset does not exist");
      require(!_minted(ballId, headId, wingId, eyeId, hatId, paletteId), "This token was already minted");
      uint tokenId = tokens.length;
      _safeMint(msg.sender, tokenId);
      tokens.push([ballId, headId, wingId, eyeId, hatId, paletteId]);
      nonOwnerTokensMinted += 1;
      return uint256(tokenId);
  }

  function mintTo(address toAddress, uint8 ballId, uint8 headId, uint8 wingId, uint8 eyeId, uint8 hatId, uint8 paletteId
  ) public payable returns(uint256) {
      require(msg.value >= 50000000000000000, "Pay at least 0.05 ETH to mint");
      require(_possibleChoice(ballId, headId, wingId, eyeId, hatId, paletteId), "Asset does not exist");
      require(!_minted(ballId, headId, wingId, eyeId, hatId, paletteId), "This token was already minted");
      uint tokenId = tokens.length;
      _safeMint(toAddress, tokenId);
      tokens.push([ballId, headId, wingId, eyeId, hatId, paletteId]);
      nonOwnerTokensMinted += 1;
      return uint256(tokenId);
  }

  function _canOwnerMint() internal view returns (bool) {
      if (tokens.length / 9 < nonOwnerTokensMinted) {
        return true;
      } else {
        return false;
      }
  }

  function _possibleChoice(uint8 ballId, uint8 headId, uint8 wingId, uint8 eyeId, uint8 hatId, uint8 paletteId) internal view returns (bool) {
    if ((ballId >= penisCollection.ballsLength()) ||
        (headId >= penisCollection.headsLength()) ||
        (wingId >= penisCollection.wingsLength()) ||
        (eyeId >= penisCollection.eyesLength()) ||
        (hatId >= penisCollection.hatsLength()) ||
        paletteId >= penisCollection.paletteLength()) {
      return false;
    }
    return true;
  }

  function _minted(uint8 ballId, uint8 headId, uint8 wingId, uint8 eyeId, uint8 hatId, uint8 paletteId) internal view returns (bool) {
    for (uint i=0; i<tokens.length; i++)
      if (ballId == tokens[i][0] &&
          headId == tokens[i][1] &&
          wingId == tokens[i][2] &&
          eyeId == tokens[i][3] &&
          hatId == tokens[i][4] &&
          paletteId== tokens[i][5]) {
        return true;
      }
    return false;
  }

  function tokenURI(uint256 id) public view virtual override returns (string memory) {
    string memory svgUri  = _encodeSvgUriAtTokenId(id);
    bytes memory json    = abi.encodePacked('{"name":"SymbolNFT #', Strings.toString(id), '","image":"', svgUri, '"}');
    string memory jsonUri = string(abi.encodePacked("data:application/json,", json));
    return jsonUri;
  }

  function _encodeSvgUriAtTokenId(uint256 id) private view returns (string memory) {
    bytes memory svg = penisCollection._getPenisSvg(tokens[id][0], tokens[id][1], tokens[id][2], tokens[id][3], tokens[id][4], tokens[id][5]);
    return string(abi.encodePacked('data:image/svg+xml,', svg));
  }

  function withdrawPayments(address payable payee) public override onlyOwner virtual {
      super.withdrawPayments(payee);
  }
}
